    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^7.16.0",
    "husky": "^7.0.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "typescript": "^4.3.2",
    "vite": "^2.4.4",
    "vue-jest": "^5.0.0-alpha.10",
